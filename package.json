{
  "name": "super_simple_stock_market",
  "version": "1.0.0",
  "private": true,
  "description": "A super_simple_stock_market using React and Redux",
  "main": "index.js",
  "scripts": {
    "build": "npm run test && webpack --config webpack.prod.js",
    "build:no-test": "webpack --config webpack.prod.js",
    "dev": "webpack-dev-server --config webpack.dev.js",
    "start": "npm run dev",
    "test": "standard && npm run unit-tests",
    "unit-tests": "mocha --recursive --require @babel/register --require ./ignore-utils",
    "unit-tests:watch": "mocha --recursive --require @babel/register --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GeorgeColdham/super_simple_stock_market.git"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.0",
    "babel-loader": "^8.0.5",
    "chai": "^4.2.0",
    "clean-webpack-plugin": "^2.0.1",
    "css-loader": "^2.1.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "file-loader": "^3.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jsdom": "^15.1.1",
    "mini-css-extract-plugin": "^0.5.0",
    "mocha": "^6.1.4",
    "node-sass": "^4.12.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "redux": "^4.0.1",
    "require-hacker": "^3.0.1",
    "sass-loader": "^7.1.0",
    "sinon": "^7.3.0",
    "standard": "^12.0.1",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "react": "^16.8.5",
    "react-dom": "^16.8.5",
    "react-redux": "^6.0.1",
    "redux-devtools-extension": "^2.13.8"
  },
  "standard": {
    "ignore": [
      "dist",
      "node_modules",
      "/test/"
    ]
  }
}
